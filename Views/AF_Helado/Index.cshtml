@model IEnumerable<FogachoWebHeladeria.Models.AF_Helado>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.AF_Nombre)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.AF_Sabor)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.AF_Categorias)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.AF_Precio)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.AF_Queso)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.AF_Nombre)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.AF_Sabor)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.AF_Categorias)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.AF_Precio)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.AF_Queso)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.AF_IdHeladeria">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.AF_IdHeladeria">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.AF_IdHeladeria">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
